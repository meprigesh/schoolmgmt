@model Student?
<h5>Do you want to delete @Model.FirstName @Model.LastName?</h5>
<div class="d-flex justify-content-start gap-2">
    <form method="post">
        <button class="btn btn-danger" type="submit">Delete</button>
    </form>
    <a class="btn btn-primary" href="/Student/List">Cancel</a>
</div>